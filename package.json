{
  "name": "traveler-front",
  "version": "0.1.0",
  "private": true,
  "description": "School project for finding travelling mate, spliting gasoline costs and what else not",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build/*",
    "start": "cubx-http-server ./build -c-1 --proxy=http://localhost:8080 & webpack --config webpack.config.js --progress --profile --watch",
    "build": "npm run clean && webpack --config webpack.config.js --progress --profile",
    "flow": "flow check",
    "test": "standard && npm run flow  && karma start --single-run --browsers PhantomJSScreenShot",
    "test-start": "karma start --auto-watch --browsers PhantomJSScreenShot"
  },
  "author": "Aleksandr Tereshchenko <aleksandr.tereshchenko@metropolia.fi>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "cross-env": "3.1.4",
    "cubx-http-server": "^0.1.2",
    "css-loader": "0.14.5",
    "eslint": "3.16.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "^2.2.7",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "file-loader": "0.10.1",
    "flow-bin": "^0.42.0",
    "html-webpack-plugin": "^2.17.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.14",
    "react-addons-perf": "^15.3.2",
    "standard": "^9.0.2",
    "style-loader": "0.13.2",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1"
  },
  "standard": {
    "ignore": [
      "build"
    ],
    "global": [
      "localStorage",
      "alert"
    ],
    "plugins": [
      "flowtype"
    ],
    "parser": "babel-eslint"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "6.23.0",
    "immutable": "^3.8.1",
    "moment": "^2.15.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  }
}
